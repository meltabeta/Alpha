{
  "version": 2,
  "routes": [
    {
      "src": "/donghua/(.*)",
      "dest": "/index.html"
    },
    {
      "src": "/anime/(.*)",
      "dest": "/index.html"
    },
    {
      "src": "/ongoing",
      "dest": "/index.html"
    },
    {
      "src": "/completed",
      "dest": "/index.html"
    },
    {
      "src": "/search",
      "dest": "/index.html"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
} 